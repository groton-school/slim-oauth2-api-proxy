{
  "name": "groton-school/slim-oauth2-api-proxy",
  "description": "Server-side actions and routes for authenticating to and accessing an REST API from a web client",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Seth Battis",
      "email": "sbattis@groton.org"
    }
  ],
  "license": "GPL-3.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "GrotonSchool\\Slim\\OAuth2\\APIProxy\\": "src/"
    }
