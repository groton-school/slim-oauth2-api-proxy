    "groton-school/slim-norms": "^1.0",
    "league/oauth2-client": "^2.8",
    "league/uri": "^7.5",
    "odan/session": "^6.4",
    "php-di/php-di": "^7.0",
    "psr/container": "^2.0",
    "psr/http-message": "^2.0",
    "slim/http": "^1.4",
    "slim/slim": "^4.12"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.76",
    "marcocesarato/php-conventional-changelog": "^1.17"
  },
  "config": {
    "sort-packages": true
  }
